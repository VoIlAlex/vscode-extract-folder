{
	"name": "extract-folder",
	"displayName": "Extract Folder",
	"description": "Utility to delete folders without deleting the content of the folders",
	"version": "1.0.2",
	"engines": {
		"vscode": "^1.42.0"
	},
	"author": {
		"name": "Ilya Vouk",
		"email": "ilya.vouk@gmail.com"
	},
	"publisher": "VoIlAlex",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.extractFolderHere"
	],
	"main": "./extension.js",
	"icon": "data/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/VoIlAlex/vscode-extract-folder"
	},
	"readme": "./README.md",
	"contributes": {
		"commands": [{
			"command": "extension.extractFolderHere",
			"title": "Extract Folder Here"
		}],
		"menus": {
			"explorer/context": [{
				"command": "extension.extractFolderHere",
				"group": "7_modification"
			}]
		},
		"configuration": {
			"title": "Extract Folder",
			"properties": {
				"extractFolder.deleteFolderAfterExtraction": {
					"type": "boolean",
					"default": true,
					"description": "Weather to delete the extracted folder or keep it."
				}
			}
		}
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.3",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}